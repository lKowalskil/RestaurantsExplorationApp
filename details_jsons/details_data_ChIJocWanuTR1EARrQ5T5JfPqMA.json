_language": "uk", "profile_photo_url": "https://lh3.googleusercontent.com/a/ACg8ocLxVo2OdK9cqy9A2rN5rSSa9n0WlC1XIzI-5Wzgp8oj=s128-c0x00000000-cc-rp-mo", "rating": 5, "relative_time_description": "5 months ago", "text": "A very cool, cozy and stylish coffee shop. A rich selection of fresh goodies, you can also come with your pets! Coffee at a height! I recommend", "time": 1695929115, "translated": true}], "serves_beer": false, "serves_vegetarian_food": true, "serves_wine": false, "takeout": true, "types": ["cafe", "store", "food", "point_of_interest", "establishment"], "url": "https://maps.google.com/?cid=13882574102647213741", "user_ratings_total": 12, "utc_offset": 120, "vicinity": "Raiduzhna Street, 25, Kyiv", "wheelchair_accessible_entrance": false}, "status": "OK"}